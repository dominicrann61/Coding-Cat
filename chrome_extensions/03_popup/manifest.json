{
    "manifest_version": 2,
    "name": "The Coding Cat Extension 3",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],

    "browser_action": {
        "default_popup": "popup.html"
    },


    "icons": {
        "128": "icon.png"

    }

}