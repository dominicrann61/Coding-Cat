{
    "manifest_version": 2,
    "name": "The Coding Cat Extension",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js"]

    },
    "browser_action": {
        "defualt_icon": "icon.png"
    },

    "icons": {
        "128": "icon.png"

    }

}