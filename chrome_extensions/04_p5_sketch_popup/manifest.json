{
    "manifest_version": 2,
    "name": "Coding Cat Fireworks Pop Up",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],

    "browser_action": {
        "default_popup": "sketch/index.html"
    },



    "icons": {
        "128": "icon.png"

    }

}